{
  "name": "Email Cat Link",
  "description": "Use options to set up categories and a default email recipient.",
  "version": "0.0.0.2",
  "icons": {
    "128": "link.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": {
    "16": "link16.png",
    "48": "link48.png",
    "128": "link128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Email this page",
    "default_icon": "link.png"
  },

  "manifest_version": 2
}